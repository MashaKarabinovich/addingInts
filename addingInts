#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int a= 1; 
    int b = 2; 
    int c = 3 ; 
    int sum; 
    
    cin >> a >> b >> c;
    sum = a + b + c;
    std::cout << sum;
    
    
    return 0;
}
